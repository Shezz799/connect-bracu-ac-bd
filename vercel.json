{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/studentdashboard", "destination": "/studentdashboard.html" },
    { "source": "/studentdashboard/gradesheet", "destination": "/gradesheet.html" },
    { "source": "/studentdashboard/advising/preregistrationphaseone", "destination": "/phase-one.html" },
    { "source": "/studentdashboard/advising/preregistrationphasetwo", "destination": "/phase-two.html" },
    { "source": "/studentdashboard/advising/sectionseatstatus", "destination": "/section-seat-status.html" },
    { "source": "/studentdashboard/scholarship/apply", "destination": "/scholarship-apply.html" },
    { "source": "/studentdashboard/scholarship/history", "destination": "/scholarship-history.html" },
    { "source": "/studentdashboard/coursedrop", "destination": "/course-drop-application.html" },
    { "source": "/studentdashboard/semesterdrop", "destination": "/semester-drop-application.html" },
    { "source": "/studentdashboard/probation", "destination": "/probation-application.html" },
    { "source": "/studentdashboard/payslip", "destination": "/payslip-view.html" },
    { "source": "/studentdashboard/myprofile", "destination": "/my-profile.html" },
    { "source": "/studentdashboard/accountsettings", "destination": "/account-settings.html" },

    { "source": "/studentdashboard/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/studentdashboard/(.*)/assets/(.*)", "destination": "/assets/$2" },

    { "source": "/studentdashboard/styles.css", "destination": "/styles.css" },
    { "source": "/studentdashboard/(.*)/styles.css", "destination": "/styles.css" },
    { "source": "/studentdashboard/dashboard.js", "destination": "/dashboard.js" },
    { "source": "/studentdashboard/(.*)/dashboard.js", "destination": "/dashboard.js" },

    { "source": "/studentdashboard/:page", "destination": "/:page.html" }
  ]
}