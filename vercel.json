{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/studentdashboard", "destination": "/studentdashboard" },
    { "source": "/studentdashboard/gradesheet", "destination": "/gradesheet" },
    { "source": "/studentdashboard/advising/preregistrationphaseone", "destination": "/phase-one" },
    { "source": "/studentdashboard/advising/preregistrationphasetwo", "destination": "/phase-two" },
    { "source": "/studentdashboard/advising/sectionseatstatus", "destination": "/section-seat-status" },
    { "source": "/studentdashboard/scholarship/apply", "destination": "/scholarship-apply" },
    { "source": "/studentdashboard/scholarship/history", "destination": "/scholarship-history" },
    { "source": "/studentdashboard/coursedrop", "destination": "/course-drop-application" },
    { "source": "/studentdashboard/semesterdrop", "destination": "/semester-drop-application" },
    { "source": "/studentdashboard/probation", "destination": "/probation-application" },
    { "source": "/studentdashboard/payslip", "destination": "/payslip-view" },
    { "source": "/studentdashboard/myprofile", "destination": "/my-profile" },
    { "source": "/studentdashboard/accountsettings", "destination": "/account-settings" },

    { "source": "/studentdashboard/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/studentdashboard/(.*)/assets/(.*)", "destination": "/assets/$2" },

    { "source": "/studentdashboard/styles.css", "destination": "/styles.css" },
    { "source": "/studentdashboard/(.*)/styles.css", "destination": "/styles.css" },
    { "source": "/studentdashboard/dashboard.js", "destination": "/dashboard.js" },
    { "source": "/studentdashboard/(.*)/dashboard.js", "destination": "/dashboard.js" },

    { "source": "/studentdashboard/:page", "destination": "/:page" }
  ]
}