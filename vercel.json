{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    { "src": "/studentdashboard", "dest": "/studentdashboard.html" },
    { "src": "/studentdashboard/gradesheet", "dest": "/gradesheet.html" },
    { "src": "/studentdashboard/advising/preregistrationphaseone", "dest": "/phase-one.html" },
    { "src": "/studentdashboard/advising/preregistrationphasetwo", "dest": "/phase-two.html" },
    { "src": "/studentdashboard/advising/sectionseatstatus", "dest": "/section-seat-status.html" },
    { "src": "/studentdashboard/scholarship/apply", "dest": "/scholarship-apply.html" },
    { "src": "/studentdashboard/scholarship/history", "dest": "/scholarship-history.html" },
    { "src": "/studentdashboard/coursedrop", "dest": "/course-drop-application.html" },
    { "src": "/studentdashboard/semesterdrop", "dest": "/semester-drop-application.html" },
    { "src": "/studentdashboard/probation", "dest": "/probation-application.html" },
    { "src": "/studentdashboard/payslip", "dest": "/payslip-view.html" },
    { "src": "/studentdashboard/myprofile", "dest": "/my-profile.html" },
    { "src": "/studentdashboard/accountsettings", "dest": "/account-settings.html" },

    { "src": "/studentdashboard/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/studentdashboard/(.*)/assets/(.*)", "dest": "/assets/$2" },

    { "src": "/studentdashboard/styles.css", "dest": "/styles.css" },
    { "src": "/studentdashboard/(.*)/styles.css", "dest": "/styles.css" },
    { "src": "/studentdashboard/dashboard.js", "dest": "/dashboard.js" },
    { "src": "/studentdashboard/(.*)/dashboard.js", "dest": "/dashboard.js" },

    { "src": "/studentdashboard/([^/]+)/?", "dest": "/$1.html" }
  ]
}